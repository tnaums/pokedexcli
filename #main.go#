package main

import (
	"time"

	"pokedexcli/internal/pokeapi"
	"pokedexcli/internal/pokecache"
)

func main() {
	pokeClient := pokeapi.NewClient(5 * time.Second)
	pokeCache := pokecache.NewCache(5 * time.Second)
	
	cfg := &config{
		pokeapiClient: pokeClient,
		pokemapCache: pokeCache,
	}

	startRepl(cfg)
}
